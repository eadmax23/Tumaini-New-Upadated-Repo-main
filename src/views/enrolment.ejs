<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="/img/favicon.png">

    <title>Enrolment</title>
    <!-- Bootstrap CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Custom styles -->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
      <script src="js/lte-ie7.js"></script>
    <![endif]-->


</head>

<body>
    <!-- container section start -->
    <section id="container" class="">
        <!--header start-->

        <%- include("./partials/header.ejs") %>
        <!--header end-->

        <!--sidebar start-->
        <%- include("./partials/aside.ejs") %>
        <!--header end-->

        <!--sidebar start-->
       <% let name = undefined %>
        <!--sidebar end-->

        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa fa-bars"></i> Pages</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="/dashboard">Home</a></li>
                            <li><i class="fa fa-bars"></i>Pages</li>
                            <li><i class="fa fa-square-o"></i>Pages</li>
                        </ol>
                    </div>
                </div>
                <!-- page start-->
                </div>
                <div class="col-md-13 portlets">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <div class="pull-left">Enrollment</div>
                            <div class="widget-icons pull-right">
                                <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a>
                                <a href="#" class="wclose"><i class="fa fa-times"></i></a>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="panel-body">
                            <div class="padd">

                                <div class="form quick-post">
                                    <!-- Edit profile form (not working)-->
                                   
                                        <form class="form-horizontal" action="/create/statistic" method="post">
                                            <!-- Students -->
                                            <div class="form-group">
                                                <label class="control-label col-lg-2" for="noStudents">Number of Students</label>
                                                <div class="col-lg-10">
                                                    <input type="number" class="form-control" id="noStudents" name="noStudents" value="" required>
                                                </div>
                                            </div>
                                            <!-- Teachers -->
                                            <div class="form-group">
                                                <label class="control-label col-lg-2" for="noTeacher">Number of Teachers</label>
                                                <div class="col-lg-10">
                                                    <input type="number" class="form-control" id="noTeacher" name="noStaffs" value="" required>
                                                </div>
                                            </div>
                                            <!-- Workers -->
                                            <div class="form-group">
                                                <label class="control-label col-lg-2" for="title">Other Workers</label>
                                                <div class="col-lg-10">
                                                    <input type="number" class="form-control" id="title" name="noWorkers" value=""  required>
                                                </div>
                                            </div>
                                            <!-- Supporters -->
                                            <div class="form-group">
                                                <label class="control-label col-lg-2" for="noSupporter">Number of Supporters</label>
                                                <div class="col-lg-10">
                                                    <input type="number" class="form-control" id="noSupporter" name="noSupporter" value="" required>
                                                </div>
                                            </div>
    
    
                                            <!-- Buttons -->
                                            <div class="form-group">
                                                <!-- Buttons -->
                                                <div class="col-lg-offset-2 col-lg-9">
                                                    <button type="submit" class="btn btn-primary">Publish</button>
    
                                                </div>
                                            </div>
                                        </form>
                                    
                                    
                                        
                                    

                                    
                                </div>
                            </div>

                            <div class="widget-foot">
                                <!-- Footer goes here -->
                            </div>

                        </div>
                    </div>

                </div>
                <!-- page end-->
            </section>
        </section>
        <!--main content end-->
        <%- include("./partials/creator.ejs") %>
    </section>
    <!-- container section end -->
    <!-- javascripts -->
    
    <%- include("./partials/script.ejs") %>


</body>

</html>