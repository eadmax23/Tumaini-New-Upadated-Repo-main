<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="/img/favicon.png">

    <title>Statistics</title>

    <!-- Bootstrap CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Custom styles -->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
      <script src="js/lte-ie7.js"></script>
    <![endif]-->


</head>

<body>
    <!-- container section start -->
    <section id="container" class="">
        <!--header start-->
        <%- include("./partials/header.ejs") %>
        <!--header end-->

        <!--sidebar start-->
        <%- include("./partials/aside.ejs") %>
        <!--sidebar end-->

        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa fa-bars"></i> Pages</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="/dasboard">Home</a></li>
                            <li><i class="fa fa-bars"></i>Pages</li>
                            <li><i class="fa fa-square-o"></i>Pages</li>
                        </ol>
                    </div>
                </div>
                <!-- page start-->

                <!-- page start-->
                <div class="">
                    <div class="col-sm-12">
                        <section class="panel">
                            <header class="panel-heading">
                                Teachers Table
                            </header>
                            <table class="table">
                                <thead>
                                    <tr>
                                        
                                        <th>Id</th>   
                                        <th>Number of Students</th>
                                        <th>Number of Supporters</th>
                                        <th>Number of Workers</th>
                                        <th>Number of teachers</th>
                                        <th colspan="1"><a href="#">update</a></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    
                                        <% if(statics) {%>
                                        <% statics.forEach((static)=> { %>
                                            <tr>
                                            <td><%= static.id%></td>
                                            <td><%= static.noStudents%></td>
                                            <td><%= static.noSupporter%></td>
                                            <td><%= static.noWorkers%></td>
                                            <td><%= static.noStaffs%></td>
                                            <td colspan="1"><a href="create/statistic/<%= static.id%>">update</a></td>
                                        </tr>
                                            <%})%>
                                        <%}%>
                                   
                                </tbody>

                            </table>
                        </section>
                    </div>
                    <!-- page end-->
            </section>
        </section>
        <!--main content end-->
        <%- include("./partials/creator.ejs") %>
    </section>
    <!-- container section end -->
    <!-- javascripts -->
    <%- include("./partials/script.ejs") %>


</body>

</html>